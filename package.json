{
  "name": "minid-onthefly-school-gui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "rimraf node_modules/.vite/ && vite --force",
    "build": "yarn locale:deploy && rimraf dist && tsc && vite build",
    "make-icons": "node ./scripts/make-icons.js",
    "locale:deploy": "yarn locale:extract && yarn locale:build",
    "locale:extract": "lit-localize extract",
    "locale:build": "lit-localize build",
    "deploy": "rimraf ../resources/public && tsc && vite build --outDir ../resources/public",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@digdir/designsystemet-css": "^0.10.0",
    "@digdir/designsystemet-theme": "^0.15.3",
    "@floating-ui/dom": "^1.6.10",
    "@vaadin/router": "^1.7.5",
    "composed-offset-position": "^0.0.6",
    "element-internals-polyfill": "^1.3.10",
    "lit": "^3.1.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@lit/localize-tools": "^0.7.1",
    "@material/web": "^1.2.0",
    "@navikt/aksel-icons": "^7.1.2",
    "@open-wc/testing": "^4.0.0",
    "@storybook/addon-essentials": "^8.3.1",
    "@storybook/addon-links": "^8.3.1",
    "@storybook/blocks": "^8.3.1",
    "@storybook/test": "^8.3.1",
    "@storybook/web-components": "^8.3.1",
    "@storybook/web-components-vite": "^8.3.1",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/user-event": "^14.5.2",
    "@types/webappsec-credential-management": "^0.6.6",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-legacy": "^5.3.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-plugin-storybook": "^0.8.0",
    "happy-dom": "^13.3.8",
    "postcss": "^8.4.31",
    "postcss-nested": "^6.0.1",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.3.3",
